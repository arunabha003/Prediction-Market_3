{
  "compilerOptions": {
    "target": "ESNext",
    "moduleResolution": "node10",
    "declaration": false,
    "rootDir": "./",
    "types": ["vitest/globals"],
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src", "tests"],
  "exclude": ["node_modules", "dist"]
}
